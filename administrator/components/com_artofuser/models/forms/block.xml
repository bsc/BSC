<?xml version="1.0" encoding="utf-8"?>
	<!-- $Id: block.xml 544 2011-01-15 04:40:24Z eddieajau $ -->
<form>
	<fields>
		<field
			name="block_catid"
			type="category"
			class="inputbox"
			size="1"
			section="com_artofuser_blocked"
			label="COM_ARTOFUSER_FIELD_BLOCK_CATID_LABEL">
			<option value="">JLIB_FORM_NO_CATEGORY</option>
		</field>

		<field
			name="note"
			type="editor"
			class="inputbox"
			label="COM_ARTOFUSER_FIELD_NOTE_LABEL"
			description="COM_ARTOFUSER_FIELD_NOTE_DESC"
			filter="safehtml"
			/>

		<field
			name="review_time"
			type="calendar"
			class="inputbox"
			label="COM_ARTOFUSER_FIELD_REVIEW_TIME_LABEL"
			description="COM_ARTOFUSER_FIELD_REVIEW_TIME_DESC"
			default="0000-00-00"
			format="%Y-%m-%d"
			/>
	</fields>

</form>